# Environment Configuration Example
# Copy this file to .env.development, .env.production, or .env.test

# Development Environment
# FLASK_ENV=development
# FLASK_DEBUG=True
# FLASK_SECRET_KEY=your-secret-key-here
# DATABASE_PATH=data/dwd_generator_dev.db
# ENVIRONMENT=development

# Production Environment
# FLASK_ENV=production
# FLASK_DEBUG=False
# FLASK_SECRET_KEY=CHANGE-THIS-TO-A-SECURE-KEY
# DATABASE_PATH=data/dwd_generator.db
# ENVIRONMENT=production

# Test Environment
# FLASK_ENV=testing
# FLASK_DEBUG=False
# FLASK_SECRET_KEY=test-secret-key
# DATABASE_PATH=data/test_database.db
# ENVIRONMENT=testing
